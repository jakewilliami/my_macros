\mode<presentation>


%Defined colors
\definecolor{TeamerGreen}{RGB}{19,78,51}
\definecolor{TeamerRed}{RGB}{78,19,33}
\definecolor{TeamerBrown}{RGB}{78,42,19}
\definecolor{TeamerLite}{RGB}{46,104,78}
\definecolor{TeamerLiteGray}{RGB}{220,220,220}

% Settings
\setbeamercolor*{title page header}{fg=black}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}


% Here are the colours for the blocks e.g. thing theorem environments.
\setbeamercolor{block title}{bg=TeamerGreen, fg=white}
\setbeamercolor{block body}{bg=TeamerLite, fg=white}


% Different blocks can be set to different colours by specifying the block name
\setbeamercolor{block title example}{bg=TeamerGreen, fg=white}
\setbeamercolor{block body example}{bg=TeamerLiteGray, fg=black}

\mode
<all>
